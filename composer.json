{
	"name": "hmdshariati/persian-nts",
	"description": "a Laravel Number to Speech package for Persian language",
	"type": "library",
	"homepage": "https://github.com/hmdshariati/persian-nts",
	"license": "MIT",
	"keywords": [
		"laravel",
		"iran",
		"persian",
		"text to speech",
		"number to speech"
	],
	"authors": [
		{
			"name": "Dena Ajdari",
			"homepage": "https://github.com/hmdshariati"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=5.5.0",
		"illuminate/support": ">=5.2.0"
	},
	"autoload": {
		"psr-4": {
			"Dena\\PersianNTS\\": "src"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Dena\\PersianNTS\\PersianNTSServiceProvider"
			]
		}
	}
}
